{
  "name": "@d3banking/eslint-config",
  "version": "22.3.0",
  "description": "D3 Banking's ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "pretest": "eslint-find-rules --unused --no-error",
    "test": "yarn --silent lint && yarn --silent tests-only"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LodoSoftware/javascript-style-guide"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "styleguide",
    "d3banking"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LodoSoftware/javascript-style-guide/issues"
  },
  "homepage": "https://github.com/LodoSoftware/javascript-style-guide",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-testing-library": "5.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.17.0",
    "@babel/plugin-transform-modules-commonjs": "7.16.8",
    "babel-tape-runner": "3.0.0",
    "eslint": "8.8.0",
    "eslint-find-rules": "4.1.0",
    "tape": "5.5.0",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "eslint": ">= 7.5.0"
  }
}
