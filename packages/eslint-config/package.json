{
  "name": "@d3banking/eslint-config",
  "version": "20.6.0",
  "description": "D3 Banking fork of Airbnb's ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "pretest": "eslint-find-rules --unused --no-error",
    "test": "yarn --silent lint && yarn --silent tests-only"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LodoSoftware/javascript-style-guide"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "airbnb",
    "javascript",
    "styleguide",
    "d3banking"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LodoSoftware/javascript-style-guide/issues"
  },
  "homepage": "https://github.com/LodoSoftware/javascript-style-guide",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "3.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "babel-preset-airbnb": "^5.0.0",
    "babel-tape-runner": "^3.0.0",
    "eslint": "7.18.0",
    "eslint-find-rules": "^3.6.1",
    "tape": "^5.1.1",
    "typescript": "4.1.3"
  },
  "peerDependencies": {
    "eslint": ">= 7.5.0"
  }
}
