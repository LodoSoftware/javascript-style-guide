{
  "name": "@d3banking/eslint-config",
  "version": "27.2.0",
  "description": "D3 Banking's ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "pretest": "eslint-find-rules --unused --no-error",
    "test": "yarn pretest && yarn lint && yarn tests-only"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LodoSoftware/javascript-style-guide"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "styleguide",
    "d3banking"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LodoSoftware/javascript-style-guide/issues"
  },
  "homepage": "https://github.com/LodoSoftware/javascript-style-guide",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "~8.5.0",
    "@typescript-eslint/parser": "~8.5.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-import-resolver-typescript": "~3.6.0",
    "eslint-plugin-import": "~2.30.0",
    "eslint-plugin-jsx-a11y": "~6.10.0",
    "eslint-plugin-react": "7.35.2",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-testing-library": "~6.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/plugin-transform-modules-commonjs": "7.24.8",
    "babel-tape-runner": "3.0.0",
    "eslint": "8.57.0",
    "eslint-find-rules": "4.1.0",
    "tape": "5.8.1",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "eslint": ">= 7.5.0"
  }
}
