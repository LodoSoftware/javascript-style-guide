{
  "name": "@d3banking/eslint-config",
  "version": "21.0.2",
  "description": "D3 Banking's ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "pretest": "eslint-find-rules --unused --no-error",
    "test": "yarn --silent lint && yarn --silent tests-only"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LodoSoftware/javascript-style-guide"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "styleguide",
    "d3banking"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LodoSoftware/javascript-style-guide/issues"
  },
  "homepage": "https://github.com/LodoSoftware/javascript-style-guide",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-import-resolver-webpack": "0.13.1",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "4.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "babel-tape-runner": "^3.0.0",
    "eslint": "7.27.0",
    "eslint-find-rules": "^3.6.1",
    "tape": "^5.2.2",
    "typescript": "4.2.4"
  },
  "peerDependencies": {
    "eslint": ">= 7.5.0"
  }
}
