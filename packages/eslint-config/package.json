{
  "name": "@d3banking/eslint-config",
  "version": "12.0.0",
  "description": "D3 Banking fork of Airbnb's ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "pretest": "eslint-find-rules --unused",
    "test": "npm run --silent lint && npm run --silent tests-only"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LodoSoftware/javascript-style-guide"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "airbnb",
    "javascript",
    "styleguide",
    "d3banking"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LodoSoftware/javascript-style-guide/issues"
  },
  "homepage": "https://github.com/LodoSoftware/javascript-style-guide",
  "dependencies": {
    "eslint-config-airbnb-base": "11.3.1",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-import": "2.7.0"
  },
  "devDependencies": {
    "babel-preset-airbnb": "^2.4.0",
    "babel-tape-runner": "^2.0.1",
    "eslint": "^4.4.1",
    "eslint-find-rules": "^3.1.1",
    "tape": "^4.8.0"
  },
  "peerDependencies": {
    "eslint": ">= 3"
  }
}
